{
  "name": "ignition-web",
  "version": "0.0.1",
  "description": "Ignition",
  "main": "index.js",
  "repository": "https://github.com/pivotalservices/ignition",
  "author": "Pivotal Software, Inc.",
  "license": "Apache-2.0",
  "private": false,
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime-corejs3": "^7.5.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.1.0",
    "html": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "prettier": "1.18.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "^16.3.2",
    "redux-devtools": "^3.4.1",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "scripts": {
    "ci": "yarn lint && yarn test && yarn build",
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --config webpack.prod.js --watch",
    "devwatch": "webpack --config webpack.dev.js --watch",
    "devbuild": "webpack --config webpack.dev.js",
    "lint": "eslint '{src,test}/**/*.js' ",
    "test": "jest --silent",
    "testwatch": "jest --watch",
    "fmt": "prettier-eslint --write '{src,test}/**/*.js'"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 3
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "dependencies": {
    "@material-ui/core": "4.3.0",
    "@material-ui/icons": "4.2.1",
    "classnames": "^2.2.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^5.0.1",
    "tar": "^4.4.8"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock-asset.js"
    },
    "testURL": "http://localhost/"
  }
}
